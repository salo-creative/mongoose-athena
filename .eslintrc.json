{
  "extends": "eslint-config-airbnb-base",
  "parser": "babel-eslint",
  "settings": {
    "ecmascript": 9
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false,
      "modules": true,
      "destructuring": true,
      "classes": true,
      "forOf": true,
      "blockBindings": true,
      "arrowFunctions": true
    }
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "camelcase": 1,
    "max-len": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "func-names": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "new-cap": 0,
    "no-undef": 2,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "quote-props": 0,
    "no-underscore-dangle": 0,
    "template-curly-spacing": [
      2,
      "always"
    ],
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "jest": true,
    "describe": true,
    "test": true,
    "expect": true,
    "it": true,
    "beforeEach": true,
    "afterEach": true,
    "beforeAll": true,
    "afterAll": true,
    "webpackVars": true
  }
}